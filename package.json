{
  "name": "lulu",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "heroku-postbuild": "knex migrate && cd client && npm install && npm run build",
    "devClient": "cd client && npm run start",
    "devServer": "nodemon bin/www",
    "dev": "concurrently --kill-others-on-fail \"npm run devServer\" \"npm run devClient\""
  },
  "dependencies": {
    "concurrently": "^3.5.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "hbs": "~4.0.1",
    "http-errors": "~1.6.2",
    "knex": "^0.14.6",
    "morgan": "~1.9.0",
    "pg": "^7.4.3"
  }
}
